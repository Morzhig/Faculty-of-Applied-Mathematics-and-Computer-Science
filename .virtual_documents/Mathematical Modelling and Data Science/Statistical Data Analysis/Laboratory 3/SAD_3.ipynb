import statsmodels.api as sm

x, y = df[['sepal length (cm)', 'petal length (cm)', 'petal width (cm)']].astype(float), df['sepal width (cm)'].astype(float)
x = sm.add_constant (x)

model = sm. OLS (y, x). fit ()
print(model. summary ())





